<h1><%= form_naming("admin.routes.form.title", @route) %></h1>

<%= render "admin/routes/stations_position", model: @route %>

<%= form_with model: [:admin, @route], local: true do |f| %>
  <%= render "components/error_block", item: @route %>

  <%= render "components/table", caption: get_caption("routes", "form") do %>
    <%= render "components/table_row" do %>
      <%= render "form_cells", f: f %>
    <% end %>
  <% end %>

  <%= render "components/buttons",
             f: f,
             form_button: form_naming("admin.routes.form.title", @route) %>
<% end %>
